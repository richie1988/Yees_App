
<h1>Employees List</h1>
<% if user_signed_in? %>
<div id="employees">
  <% @employees.each do |employee| %>
    <%= render employee %>
    <p>
      <%= link_to "Show this employee", employee %>
    </p>
  <% end %>
</div>

<%= link_to "New employee", new_employee_path, class:"btn btn-secondary" %>
<% else %>
<h1>Hello World!</h1>
This is my first Rails webapage

<% end %>
